  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "predeploy": "expo export -p web --output-dir dist && powershell -Command \"Copy-Item dist\\index.html dist\\404.html -Force; if (Test-Path public) { Copy-Item -Recurse -Force public\\* dist\\ }\" && node scripts/inject-manifest.js",
    "deploy": "gh-pages --nojekyll -d dist"
  },
  "dependencies": {
    "@expo/metro-config": "^54.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "expo": "^54.0.2",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.12",
    "expo-sharing": "~14.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-web": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  }
}
